{
  "parser": "babel-eslint",
  "plugins": [
    "flow-vars"
  ],
  "ecmaFeatures": {
    "modules": true
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "rules": {
    "quotes": [2, "single", "avoid-escape"],
    "no-underscore-dangle": [0],
    "no-dupe-class-members": [0]
  }
}
