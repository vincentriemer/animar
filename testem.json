{
  "framework": "mocha",
  "before_tests": "webpack --config webpack.config.test.js",
  "after_tests": "rm -rf browser-test",
  "src_files": [
    "test/*.js"
  ],
  "timeout": 900,
  "parallel": 5,
  "port": 7000,
  "on_start": "./sauce-scripts/saucie-connect.js",
  "on_exit": "./sauce-scripts/saucie-disconnect.js",
  "serve_files": [
    "node_modules/chai/chai.js",
    "node_modules/sinon/pkg/sinon.js",
    "test-scripts/browser-test.js",
    "browser-test/*.js"
  ],
  "launchers": {
    "node": {
      "command": "./node_modules/.bin/babel-node ./node_modules/.bin/isparta cover test-scripts/node-test.js",
      "protocol": "tap"
    },
    "SL_Chrome_Current": {
      "exe": "./node_modules/.bin/saucie",
      "args": ["-b", "chrome", "--no-ct", "--at", "-u"],
      "protocol": "browser"
    },
    "SL_Firefox_Current": {
      "exe": "./node_modules/.bin/saucie",
      "args": ["-b", "firefox", "-v", "42", "--no-ct", "--at", "-u"],
      "protocol": "browser"
    },
    "SL_Safari_Current": {
      "exe": "./node_modules/.bin/saucie",
      "args": ["-b", "safari", "-v", "9", "--no-ct", "--at", "-u"],
      "protocol": "browser"
    },
    "SL_Safari_Last": {
      "exe": "./node_modules/.bin/saucie",
      "args": ["-b", "safari", "-v", "8", "--no-ct", "--at", "-u"],
      "protocol": "browser"
    },
    "SL_MS_Edge": {
      "exe": "./node_modules/.bin/saucie",
      "args": ["-b", "microsoftedge", "--no-ct", "--at", "-u"],
      "protocol": "browser"
    },
    "SL_IE_11": {
      "exe": "./node_modules/.bin/saucie",
      "args": ["-b", "internet explorer", "-v", "11", "--no-ct", "--at", "-u"],
      "protocol": "browser"
    },
    "SL_iOS_Current": {
      "exe": "./node_modules/.bin/saucie",
      "args": ["-b", "iphone" ,"-p", "OS X 10.10", "-v", "9.1", "--dn", "iPhone 6", "--do=portrait", "--no-ct", "--at", "-u"],
      "protocol": "browser"
    },
    "SL_Android_Current": {
      "exe": "./node_modules/.bin/saucie",
      "args": ["-b", "android" ,"-p", "Linux", "-v", "5.1", "--dn=Android Emulator", "--do=portrait", "--no-ct", "--at", "-u"],
      "protocol": "browser"
    },
    "SL_iOS_Last": {
      "exe": "./node_modules/.bin/saucie",
      "args": ["-b", "iphone" ,"-p", "OS X 10.10", "-v", "8.4", "--dn='iPhone 6'", "--do=portrait", "--no-ct", "--at", "-u"],
      "protocol": "browser"
    },
    "SL_Android_Last": {
      "exe": "./node_modules/.bin/saucie",
      "args": ["-b", "android" ,"-p", "Linux", "-v", "4.4", "--dn=Android Emulator", "--do=portrait", "--no-ct", "--at", "-u"],
      "protocol": "browser"
    }
  },
  "launch_in_dev": [
    "node",
    "PhantomJS"
  ],
  "launch_in_ci": [
    "SL_Chrome_Current",
    "SL_Firefox_Current",
    "SL_Safari_Current",
    "SL_Safari_Last",
    "SL_MS_Edge",
    "SL_IE_11",
    "SL_iOS_Current",
    "SL_iOS_Last"
  ]
}