{
  "name": "animar",
  "version": "0.8.0",
  "description": "a modern, focused, flexible javascript animation library.",
  "keywords": [
    "web",
    "javascript",
    "animation",
    "additive"
  ],
  "author": {
    "name": "Vincent Riemer",
    "email": "vincentriemer@gmail.com",
    "url": "http://vincentriemer.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vincentriemer/animar"
  },
  "bugs": {
    "url": "https://github.com/vincentriemer/animar/issues",
    "email": "vincentriemer+animar@gmail.com"
  },
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist",
    "clean-coverage": "rimraf coverage",
    "clean-lib": "rimraf lib",
    "build": "./scripts/build.sh",
    "prebuild": "npm run clean && npm run prepublish",
    "lint": "eslint src/** test/**",
    "typecheck": "./node_modules/.bin/flow check",
    "test": "NODE_ENV=test ./node_modules/.bin/babel-node ./node_modules/.bin/isparta cover test-scripts/node-test.js",
    "pretest": "npm run clean-coverage && npm run lint && npm run typecheck",
    "browser-test": "NODE_ENV=test testem ci",
    "prepublish": "npm run clean-lib && ./node_modules/.bin/babel ./src --out-dir ./lib",
    "doc": "esdoc -c esdoc.json"
  },
  "pre-commit": [
    "test",
    "build"
  ],
  "devDependencies": {
    "babel-cli": "^6.2.0",
    "babel-core": "^6.4.5",
    "babel-eslint": "^4.1.5",
    "babel-loader": "^6.2.0",
    "babel-plugin-flow-comments": "^6.3.19",
    "babel-plugin-rewire": "^1.0.0-beta-2",
    "babel-plugin-transform-class-properties": "^6.2.2",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.6.2",
    "babel-plugin-transform-flow-strip-types": "^6.3.15",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-es2015-loose": "^7.0.0",
    "chai": "^3.4.1",
    "compression-webpack-plugin": "^0.3.0",
    "core-js": "^2.0.2",
    "esdoc": "^0.4.3",
    "esdoc-es7-plugin": "^0.0.3",
    "esdoc-importpath-plugin": "0.0.1",
    "eslint": "^1.10.1",
    "eslint-plugin-flowtype": "^1.0.0",
    "flow-bin": "^0.20.1",
    "isparta": "^4.0.0",
    "jsdom": "^8.0.1",
    "lodash": "^4.0.0",
    "mocha": "^2.4.1",
    "pre-commit": "^1.1.2",
    "rewire": "^2.5.1",
    "rimraf": "^2.3.1",
    "saucie": "^1.3.1",
    "sinon": "^2.0.0-pre",
    "testem": "^1.0.0",
    "travis-weigh-in": "^1.0.2",
    "tsd": "^0.6.3",
    "webpack": "^1.12.8"
  }
}
