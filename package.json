{
  "name": "animar",
  "description": "a modern, focused, flexible javascript animation library.",
  "keywords": [
    "web",
    "javascript",
    "animation",
    "additive"
  ],
  "author": {
    "name": "Vincent Riemer",
    "email": "vincentriemer@gmail.com",
    "url": "http://vincentriemer.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vincentriemer/animar"
  },
  "bugs": {
    "url": "https://github.com/vincentriemer/animar/issues",
    "email": "vincentriemer+animar@gmail.com"
  },
  "license": "MIT",
  "main": "dist/animar.js",
  "esnext:main": "dist/animar.es6.js",
  "scripts": {
    "clean": "npm run clean:dist && npm run clean:coverage && npm run clean:lib && npm run clean:docs",
    "clean:dist": "rimraf dist",
    "clean:coverage": "rimraf coverage",
    "clean:docs": "rimraf docs",
    "build": "npm run build:umd && npm run build:es6 && npm run build:production",
    "build:umd": "rollup -c rollup.umd.config.js",
    "build:es6": "rollup -c rollup.es6.config.js",
    "build:production": "rollup -c rollup.production.config.js",
    "build:test": "rollup -c rollup.test.config.js",
    "prebuild": "npm run clean:dist",
    "postbuild": "npm run commit:artifacts",
    "lint": "eslint src/** test/**",
    "test": "./node_modules/.bin/babel-node ./node_modules/.bin/isparta cover test-scripts/node-test.js",
    "test:browser": "testem ci",
    "pretest": "npm run clean:coverage && npm run lint",
    "prepublish": "npm run clean:dist && npm run build",
    "docs": "esdoc -c esdoc.json",
    "predoc": "npm run clean:docs",
    "commit": "git-cz",
    "commit:artifacts": "git add dist",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "pre-commit": [
    "test",
    "build"
  ],
  "devDependencies": {
    "babel-cli": "^6.2.0",
    "babel-core": "^6.2.1",
    "babel-eslint": "^4.1.5",
    "babel-loader": "^6.2.0",
    "babel-plugin-transform-class-properties": "^6.2.2",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.2.0",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-es2015-loose": "^6.1.3",
    "babel-preset-es2015-rollup": "^1.0.0",
    "chai": "^3.4.1",
    "commitizen": "^2.4.6",
    "compression-webpack-plugin": "^0.2.0",
    "core-js": "^1.2.6",
    "cz-conventional-changelog": "^1.1.5",
    "esdoc": "^0.4.3",
    "esdoc-es7-plugin": "^0.0.3",
    "eslint": "^1.10.1",
    "eslint-plugin-flowtype": "^1.0.0",
    "isparta": "^4.0.0",
    "jsdom": "^7.0.2",
    "lodash": "^3.3.1",
    "mocha": "^2.3.4",
    "pre-commit": "^1.1.2",
    "rimraf": "^2.3.1",
    "rollup": "^0.24.1",
    "rollup-plugin-babel": "^2.3.8",
    "rollup-plugin-commonjs": "^2.1.0",
    "rollup-plugin-multi-entry": "^1.0.1",
    "rollup-plugin-npm": "^1.2.1",
    "rollup-plugin-uglify": "^0.1.0",
    "saucie": "^1.3.1",
    "semantic-release": "^4.3.5",
    "sinon": "^2.0.0-pre",
    "testem": "^0.9.11",
    "travis-weigh-in": "^1.0.2",
    "webpack": "^1.12.8"
  },
  "czConfig": {
    "path": "node_modules/cz-conventional-changelog"
  }
}
