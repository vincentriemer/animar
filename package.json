{
  "name": "animar",
  "version": "0.5.4",
  "description": "a modern, focused, flexible javascript animation library.",
  "keywords": [
    "web",
    "javascript",
    "animation",
    "additive"
  ],
  "author": {
    "name": "Vincent Riemer",
    "email": "vincentriemer@gmail.com",
    "url": "http://vincentriemer.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vincentriemer/animar"
  },
  "bugs": {
    "url": "https://github.com/vincentriemer/animar/issues",
    "email": "vincentriemer+animar@gmail.com"
  },
  "license": "MIT",
  "scripts": {
    "clean": "rimraf dist",
    "clean-coverage": "rimraf coverage",
    "clean-lib": "rimraf lib",
    "build": "./scripts/build.sh",
    "prebuild": "npm run clean",
    "lint": "eslint src",
    "typecheck": "./node_modules/.bin/flow check",
    "test": "./node_modules/.bin/babel-node ./node_modules/.bin/isparta cover test-scripts/node-test.js",
    "pretest": "npm run clean-coverage && npm run lint && npm run typecheck",
    "dev-test": "./node_modules/.bin/testem",
    "ci-test": "./node_modules/.bin/testem ci --port 8080",
    "prepublish": "npm run clean-lib && ./node_modules/.bin/babel ./src --out-dir ./lib"
  },
  "main": "lib/animar.js",
  "devDependencies": {
    "babel-cli": "^6.2.0",
    "babel-core": "^6.2.1",
    "babel-eslint": "^4.1.5",
    "babel-loader": "^6.2.0",
    "babel-plugin-transform-class-properties": "^6.2.2",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.2.0",
    "babel-plugin-transform-flow-strip-types": "^6.1.18",
    "babel-preset-es2015": "^6.1.18",
    "chai": "^3.4.1",
    "compression-webpack-plugin": "^0.2.0",
    "eslint": "^1.10.1",
    "eslint-plugin-flow-vars": "^0.1.0",
    "flow-bin": "^0.18.1",
    "isparta": "^4.0.0",
    "jsdom": "^7.0.2",
    "lodash": "^3.3.1",
    "mocha": "^2.3.4",
    "modernizr": "^3.2.0",
    "rimraf": "^2.3.1",
    "saucie": "^1.3.1",
    "sinon": "^1.15.4",
    "testem": "^0.9.11",
    "travis-weigh-in": "^1.0.2",
    "tsd": "^0.6.3",
    "webpack": "^1.12.8"
  }
}
